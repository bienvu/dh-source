<div class="landing-wrap">
  <div class="landing">
    <div class="landing__left ms-left ms-easing">
      <div class="ms-group">
  			{% for item in group_left %}
  				<div class="ms-section" style="background-color: {{ item.bgcolor }}">
  					{% if item.landing_image is not empty %}
  						<div class="group-content with-image title-center">
  							{% if item.landing_title %}
  			          <h1 class="title--center">{{ item.landing_title }}</h1>
  							{% endif %}
  							{% if item.landing_image %}
  		          	<img src="{{ item.landing_image.url }}" title="{{ item.landing_image.title }}">
  							{% endif %}
  							{% if item.landing_link %}
  		          	<a href="{{ item.landing_link }}" class="link-more prev" data-page-id="{{ fn('url_to_postid', item.landing_link) }}" data-page-trans="slide-in-from-right" data-start-load="sjc-start" data-page-name="sjc-loaded">Prev</a>
  							{% endif %}
  		        </div>
  					{% else %}
  						<div class="group-content">
  							{% if item.landing_title %}
  								<h2 class="text--right">{{ item.landing_title }}</h2>
  							{% endif %}
  							{% if item.email %}
  								<h3><a href="mailto:{{item.email}}">{{ item.email }}</a></h3>
  							{% endif %}
  							{% if item.address %}
  								{{ item.address }}
  							{% endif %}
  						</div>
  					{% endif %}
  	      </div>
  			{% endfor %}
      </div>
    </div>
    <div class="landing__right ms-right ms-easing">
      <div class="ms-group">
  			{% for item in group_right %}
  				<div class="ms-section" style="background-color: {{ item.bgcolor }}">
  					{% if item.landing_image is not empty %}
  						<div class="group-content with-image">
  							{% if item.landing_title %}
  								<h1 class="title--bottom">{{ item.landing_title }}</h1>
  							{% endif %}
  							{% if item.landing_image %}
  								<img src="{{ item.landing_image.url }}" title="{{ item.landing_image.title }}">
  							{% endif %}
  							{% if item.landing_link %}
  								<a href="{{ item.landing_link }}" class="link-more prev" data-page-id="{{ fn('url_to_postid', item.landing_link) }}" data-page-trans="slide-in-from-left" data-start-load="denholm-start" data-page-name="denholm-loaded">Prev</a>
  							{% endif %}
  						</div>
  					{% else %}
  						<div class="group-content">
  							{% if item.landing_title %}
  								<h2 class="text--left">{{ item.landing_title }}</h2>
  							{% endif %}
  							{% if item.email %}
  								<h3><a href="mailto:{{item.email}}">{{ item.email }}</a></h3>
  							{% endif %}
  							{% if item.address %}
  								{{ item.address }}
  							{% endif %}
  						</div>
  					{% endif %}
  				</div>
  			{% endfor %}
      </div>
    </div>
  </div>
  <div class="page-ajaxload"></div>
</div>
